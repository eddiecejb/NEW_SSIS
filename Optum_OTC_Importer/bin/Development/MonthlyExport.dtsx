      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{3E623206-18B8-4D6D-9AFA-389A64C0BAFC}"
          SQLTask:SqlStatementSource="insert into Export_Encounters_Data_History&#xA;select '0', *, GETDATE() from Export_Encounters_Data&#xA;truncate table dbo.Export_Encounters_Data&#xA;&#xA;&#x9;  select od.order_id, sum(od.Quantity_Shipped * p.Total_Member_Cost) as Order_Total&#xA;&#x9;  into #temp_dms_orders&#xA;&#x9;  from Order_Details od (nolock) &#xA;&#x9;  join Orders o (nolock) on o.Order_ID = od.Order_ID&#xA;&#x9;  join Products p (nolock) on od.Product_ID = p.Product_ID &#xA;&#x9;  where Order_Type = '4' or o.Total_Item_Cost is null &#xA;&#x9;  group by od.Order_ID &#xA;&#x9;  &#xA;&#x9;  &#xA;&#x9;   insert into dbo.Export_Encounters_Data&#xA;&#x9;   SELECT &#xA;&#x9;   --   ROW_NUMBER() &#xA;&#x9;&#x9;&#x9; --OVER (ORDER BY o.Order_Id) AS Row&#xA;&#x9;    'DTL' [Record_Type]&#xA;&#x9;    ,replace(convert(varchar(10),GETDATE(),101) ,'/','') [Created_date]&#xA;&#x9;    ,'109' as [Action_code]&#xA;&#x9;    ,' ' as [Filler_1]&#xA;&#x9;    ,replace(LEFT(m.External_Member_ID,12), 'M','') as [Subscriber_ID]&#xA;&#x9;    ,'' as [Subid_alt]&#xA;&#x9;    ,m.HIC_Number as [Member_Medicaid_ID]&#xA;&#x9;    ,upper(LEFT(m.First_Name,25)) as [Subscriber_first_name]&#xA;&#x9;    ,upper(LEFT(m.Last_Name,35)) as [Subscriber_last_name]&#xA;&#x9;    ,upper(LEFT(mal.Address_1,40)) as [Subscriber_address_1]&#xA;&#x9;    ,upper(LEFT(mal.Address_2,40)) as [Subscriber_address_2]&#xA;&#x9;    ,upper(LEFT(mal.City,25)) as [Subscriber_city]&#xA;&#x9;    ,upper(mal.State) as [Subscriber_state]&#xA;&#x9;    ,upper(replace(mal.Zip,'-','')) as [Subscriber_zip]&#xA;&#x9;    ,upper(m.First_Name) as [Patient_first_name]&#xA;&#x9;    ,upper(m.Last_Name) as [Patient_last_name]&#xA;&#x9;    ,'0' as [Member_Number]&#xA;&#x9;    ,'M' as [Member_Relation]&#xA;&#x9;    ,replace(convert(varchar(10),M.DOB,101) ,'/','') as [Birth_date]&#xA;&#x9;    ,M.Gender as [Gender]&#xA;&#x9;    ,'' as [Fillerr_2]&#xA;&#x9;    ,'411289245' as [Provider_Tax_id]&#xA;&#x9;    ,'33-0441200' as [Provider_number]&#xA;&#x9;    ,'' as [Provider_MAID]&#xA;&#x9;    ,'' as [Servicing_Provider_NPI]&#xA;&#x9;    ,'' as [Provider_name]&#xA;&#x9;    ,'' as [Provider_Address1]&#xA;&#x9;    ,'' as [Provider_Address2]&#xA;&#x9;    ,'' as [Provider_city]&#xA;&#x9;    ,'' as [Provider_state]&#xA;&#x9;    ,'' as [Provider_zip]&#xA;&#x9;    ,'' as [Provider_Specialty]&#xA;&#x9;    ,'12' as [Place_of_Service]&#xA;&#x9;    ,'' as [Referring_Provider_Number]&#xA;&#x9;    ,'33-0441200' as [Billing_Provider_Number]&#xA;&#x9;    ,'' as [Fillerr_3]&#xA;&#x9;    ,'' as [Group_number] &#xA;&#x9;    --,case when o.dt_order &lt; '01/01/2016' then '2' + RIGHT(gc.Group_code,5) else  'H2226' + RIGHT(gc.Group_code,3) end as [Group_Policy_Number] -- claims before 1/1/2016 need to be actual group code&#xA;&#x9;    ,'H2226001' as [Group_Policy_Number]&#xA;&#x9;    ,'MA01' as [Subgroup_Number]&#xA;&#x9;    ,'MA01' as [Subgroup_Policy_Number]&#xA;&#x9;    ,'' as [System]&#xA;&#x9;    ,'MASCO' as [Plan_number]&#xA;&#x9;    ,'' as [Fillerr_4]&#xA;&#x9;    ,CAST(O.Order_ID AS CHAR(12)) as [Claim_number]&#xA;&#x9;    ,'' as [Legacy_claim_code]&#xA;&#x9;    ,replace(convert(varchar(10),o.DT_Order,101) ,'/','') as [Received_date]&#xA;&#x9;    ,replace(convert(varchar(10),o.DT_Print,101) ,'/','') as [Process_date]&#xA;&#x9;    ,case when o.dt_order &lt; '10/1/2015' then '7999' else 'R99' end as [Diagnosis_1] -- BEFORE 10/1/2015 = 7999 AFTER 10/1/2015 = R99&#xA;&#x9;    ,'' as [Diagnosis_2]&#xA;&#x9;    ,'' as [Diagnosis_3]&#xA;&#x9;    ,'' as [Diagnosis_4]&#xA;&#x9;    ,replace(convert(varchar(10),o.DT_Order,112) ,'/','') as [Service_Thru_Date]&#xA;&#x9;    ,'' as [Preauthorization_Number]&#xA;&#x9;&#x9;,CASE WHEN o.Total_Item_Cost is null or o.Total_Item_Cost = 0 then REPLICATE('0',10-LEN(REPLACE(cast(t.order_total as varchar),'.',''))) + REPLACE(cast(t.order_total as varchar),'.','') &#xA;&#x9;&#x9;&#x9;  ELSE REPLICATE('0',10-LEN(REPLACE(cast(o.Total_Item_Cost as varchar),'.',''))) + REPLACE(cast(o.Total_Item_Cost as varchar),'.','') &#xA;&#x9;&#x9;END as [Claim_Charge_Amount]&#xA;&#x9;    ,'' as [Claim_Interest_Paid]&#xA;&#x9;    --,REPLICATE('0',10-LEN(REPLACE(cast(o.Total_Item_Cost as varchar),'.',''))) + REPLACE(cast(o.Total_Item_Cost as varchar),'.','') as [Claim_Paid_Amount]&#xA;&#x9;&#x9;,CASE WHEN o.Total_Item_Cost is null or o.Total_Item_Cost = 0 then REPLICATE('0',10-LEN(REPLACE(cast(t.order_total as varchar),'.',''))) + REPLACE(cast(t.order_total as varchar),'.','') &#xA;&#x9;&#x9;&#x9;  ELSE REPLICATE('0',10-LEN(REPLACE(cast(o.Total_Item_Cost as varchar),'.',''))) + REPLACE(cast(o.Total_Item_Cost as varchar),'.','') &#xA;&#x9;&#x9;END&#x9;as [Claim_Paid_Amount]   &#xA;&#x9;    ,'I' as [Network_Indicator]&#xA;&#x9;    ,'N' as [Auto_Adjudicated]&#xA;&#x9;    ,'' as [Accident_Indicator]&#xA;&#x9;    ,'' as [Accident_Symptom_Date]&#xA;&#x9;    ,'' as [Employment_Related_Indicator]&#xA;&#x9;    ,'' as [Accept_Assignment_Indicator]&#xA;&#x9;    ,'' as [Other_Coverage_Indicator]&#xA;&#x9;    ,cast(ROW_NUMBER() OVER(PARTITION BY O.[Order_ID] ORDER BY OD.[Order_Detail_ID] DESC) as CHAR(2)) AS [Line_Number]&#xA;&#x9;   -- ,'0' as [Line_Number] -- cast(ROW_NUMBER() OVER (ORDER BY o.Order_Id) as CHAR(2)) as [Line_number]&#xA;&#x9;    ,replace(convert(varchar(10),o.DT_Order,112) ,'/','') as [Service_date]&#xA;&#x9;    ,isnull(pds.Hcpc_Code,'A9999') as [Procedure_code]  -- MAP TO PRODUCT OPTIONS CHECK FOR NULL VALUES Some of these are null&#xA;&#x9;    ,'' as [Modifier_1]&#xA;&#x9;    ,'' as [Modifier_2]&#xA;&#x9;    ,'' as [Modifier_3]&#xA;&#x9;    ,'' as [Modifier_4]&#xA;&#x9;    ,'' as [Fillerr_5]&#xA;&#x9;    ,'' as [Tooth_Number]&#xA;&#x9;    ,'' as [Tooth_Number_Beginning]&#xA;&#x9;    ,'' as [Tooth_Number_Ending]&#xA;&#x9;    ,'' as [Surface1]&#xA;&#x9;    ,'' as [Surface2]&#xA;&#x9;    ,'' as [Surface3]&#xA;&#x9;    ,od.Quantity_Shipped as [Units]&#xA;&#x9;    ,'' as [Percent_Coinsurance]&#xA;&#x9;    ,'' as [Fillerr_6]&#xA;&#x9;    ,REPLICATE('0',10-LEN(REPLACE(cast(od.Quantity_Shipped * p.Total_Member_Cost as varchar),'.',''))) + REPLACE(cast(od.Quantity_Shipped * p.Total_Member_Cost as varchar),'.','') as [Service_amount_claimed]&#xA;&#x9;    ,REPLICATE('0',10-LEN(REPLACE(cast(od.Quantity_Shipped * p.Total_Member_Cost as varchar),'.',''))) + REPLACE(cast(od.Quantity_Shipped * p.Total_Member_Cost as varchar),'.','') as [Service_amount_allowed]&#xA;&#x9;    ,'0000000000' as [Service_amount_not_allowed]&#xA;&#x9;    ,'0000000000' as [Deductible]&#xA;&#x9;    ,'0000000000' as [Coinsurance]&#xA;&#x9;    ,'0000000000' as [Other_insurance_paid]&#xA;&#x9;    ,REPLICATE('0',10-LEN(REPLACE(cast(od.Quantity_Shipped * p.Total_Member_Cost as varchar),'.',''))) + REPLACE(cast(od.Quantity_Shipped * p.Total_Member_Cost as varchar),'.','') as [Service_paid_amount]&#xA;&#x9;    ,'0000000000' as [PPO_Savings]&#xA;&#x9;    ,'0000000000' as [Amount_over_max]&#xA;&#x9;    ,'' as [Fillerr_7]&#xA;&#x9;    ,'0000000000' as [Adjusted_Amount_claimed]&#xA;&#x9;    ,'0000000000' as [Adjusted_Amount_allowed]&#xA;&#x9;    ,'0000000000' as [Amount_not_allowed]&#xA;&#x9;    ,'0000000000' as [Adjusted_Deductible]&#xA;&#x9;    ,'0000000000' as [Adjusted_Coinsurance]&#xA;&#x9;    ,'0000000000' as [Adjusted_Other_insurance_paid]&#xA;&#x9;    ,'0000000000' as [Adjusted_Paid_amount]&#xA;&#x9;    ,'' as [Fillerr_8]&#xA;&#x9;    ,'' as [Disallow_EOB_Code]&#xA;&#x9;    ,'' as [Limit_Disallow_EOB_Code]&#xA;&#x9;    ,'' as [Category_Disallow_EOB_Code]&#xA;&#x9;    ,'' as [Frequency_Disallow_EOB_Code]&#xA;&#x9;    ,'' as [Manual_EOB_Code]&#xA;&#x9;    ,'' as [Disallow_EOB_Description]&#xA;&#x9;    ,'' as [Limit_Disallow_EOB_Description]&#xA;&#x9;    ,'' as [Category_Disallow_EOB_Description]&#xA;&#x9;    ,'' as [Frequency_Disallow_EOB_Description]&#xA;&#x9;    ,'' as [Manual_EOB_Description]&#xA;&#x9;    ,'' as [Fillerr_9]&#xA;&#x9;    ,'' as [Check_number]&#xA;&#x9;    ,replace(convert(varchar(10),o.DT_Order,101) ,'/','') as [Check_issue_date]&#xA;&#x9;&#x9;,CASE WHEN o.Total_Item_Cost is null or o.Total_Item_Cost = 0 then REPLICATE('0',10-LEN(REPLACE(cast(t.order_total as varchar),'.',''))) + REPLACE(cast(t.order_total as varchar),'.','') &#xA;&#x9;&#x9;&#x9;  ELSE REPLICATE('0',10-LEN(REPLACE(cast(o.Total_Item_Cost as varchar),'.',''))) + REPLACE(cast(o.Total_Item_Cost as varchar),'.','') &#xA;&#x9;&#x9;END&#x9;as [Check_Amount]&#xA;&#x9;    --,REPLICATE('0',10-LEN(REPLACE(cast(o.Total_Item_Cost as varchar),'.',''))) + REPLACE(cast(o.Total_Item_Cost as varchar),'.','')  as [Check_Amount]&#xA;&#x9;    ,'' as [Check_Void_Date]&#xA;&#x9;    ,'' as [Copay]&#xA;&#x9;    ,'' as [Adjusted_Copay]&#xA;&#x9;    ,'' as [Reissued_Flag]&#xA;&#x9;    ,'' as [Emergency_Services_Ind__]&#xA;&#x9;    ,'' as [EPSDT]&#xA;&#x9;    ,'C' as [Claim_Status] -- verifiy value (c or n for non-capitated)&#xA;&#x9;    ,'' as [Fillerr_10]&#xA;&#x9;    ,case when o.dt_order &lt; '10/01/2015' then '9' else '0' end as [Diagnosis_Ind] -- 9 before 10/1 0 after 10/1&#xA;&#x9;    ,'' as [Diagnosis_5]&#xA;&#x9;    ,'' as [Diagnosis_6]&#xA;&#x9;    ,'' as [Diagnosis_7]&#xA;&#x9;    ,'' as [Diagnosis_8]&#xA;&#x9;    ,'' as [Diagnosis_9]&#xA;&#x9;    ,'' as [Diagnosis_10]&#xA;&#x9;    ,'' as [Diagnosis_11]&#xA;&#x9;    ,'' as [Diagnosis_12]&#xA;&#x9;    ,'1' as [Diagnosis_Pointer_1]&#xA;&#x9;    ,'' as [Diagnosis_Pointer_2]&#xA;&#x9;    ,'' as [Diagnosis_Pointer_3]&#xA;&#x9;    ,'' as [Diagnosis_Pointer_4]&#xA;&#x9;    ,'' as [Policy_Number]&#xA;&#x9;    ,'' as [Carrier_Code]&#xA;&#x9;    ,'' as [Carrier_Name]&#xA;&#x9;    ,'' as [Other_Payer_Paid_Date]&#xA;&#x9;    ,'' as [Other_Insurance_Allowed_Amount]&#xA;&#x9;    ,'' as [Other_Insurance_CoPay]&#xA;&#x9;    ,'' as [Other_Insurance_CoInsurance]&#xA;&#x9;    ,'' as [Other_Insurance_Deductible]&#xA;&#x9;    ,'' as [Other_Insurance_Adjustment_Reason_Code]&#xA;&#x9;    ,'' as [Other_Insurance_Remit_Code]&#xA;&#x9;    ,REPLICATE('0',15-LEN(CAST(od.quantity_shipped as varchar))) + CAST(od.quantity_shipped as varchar) as [Quantity]&#xA;&#x9;    ,'' as [Condition_Code_Category_A]&#xA;&#x9;    ,'' as [Condition_Code_Indicator_A1]&#xA;&#x9;    ,'' as [Condition_Code_Indicator_A2]&#xA;&#x9;    ,'' as [Condition_Code_Indicator_A3]&#xA;&#x9;    ,'' as [Condition_Code_Indicator_A4]&#xA;&#x9;    ,'' as [Condition_Code_Indicator_A5]&#xA;&#x9;    ,'' as [Condition_Code_Category_B]&#xA;&#x9;    ,'' as [Condition_Code_Indicator_B1]&#xA;&#x9;    ,'' as [Condition_Code_Indicator_B2]&#xA;&#x9;    ,'' as [Condition_Code_Indicator_B3]&#xA;&#x9;    ,'' as [Condition_Code_Indicator_B4]&#xA;&#x9;    ,'' as [Condition_Code_Indicator_B5]&#xA;&#x9;    ,'' as [Condition_Code_Category_C]&#xA;&#x9;    ,'' as [Condition_Code_Indicator_C1]&#xA;&#x9;    ,'' as [Condition_Code_Indicator_C2]&#xA;&#x9;    ,'' as [Condition_Code_Indicator_C3]&#xA;&#x9;    ,'' as [Condition_Code_Indicator_C4]&#xA;&#x9;    ,'' as [Condition_Code_Indicator_C5]&#xA;&#x9;    ,isnull(gc.Pcp_id,'') as [PCP_Id] -- NEED TO FIND WHERE TO STORE THIS INFORMATION FROM IMPORT FILE.&#xA;&#x9;    ,m.member_id as [Patient_Account_Number]&#xA;&#x9;    ,'' as [Health_Insurance_Claim_(HIC)_number]&#xA;&#x9;    ,'' as [CAID/CARE_Indicator]&#xA;&#x9;    ,'' as [Claim_Submission_Media_type]&#xA;&#x9;    ,'' as [Medicare_Paid_Amount]&#xA;&#x9;    ,'' as [Medicare_Carrier_Code]&#xA;&#x9;    ,'' as [Medicare_Carrier_Name]&#xA;&#x9;    ,'' as [Fillerr_11]&#xA;&#x9;--into #TEMP_EXPORT&#xA;&#x9;from &#xA;&#x9;&#x9;  Orders o (nolock) &#xA;&#x9;left join &#xA;&#x9;&#x9;  Order_Details od (nolock) on o.Order_ID = od.Order_ID &#xA;&#x9;left join&#xA;&#x9;&#x9;  Member_Benefit_Events mbe (nolock) on o.Benefit_Event_ID = mbe.Benefit_Event_ID&#xA;&#x9;left join&#xA;&#x9;&#x9;  Members m (nolock) on o.Member_ID = m.Member_ID &#xA;&#x9;left join &#xA;&#x9;&#x9;  Member_Address_List mal (nolock) on m.Current_Address_ID = mal.Address_ID&#xA;&#x9;outer apply &#xA;&#x9;&#x9;  (select top 1 group_code, pcp_id from Members_Group_Codes mgc (nolock) where mgc.External_Member_Id = m.External_Member_ID order by DT_Updated desc) gc&#xA;&#x9;left join &#xA;&#x9;&#x9;  Products p (nolock) on od.Product_ID = p.product_id&#xA;&#x9;left join &#xA;&#x9;&#x9;  Product_Dropship_Item_Options pds (nolock) on p.Product_Code = pds.Product_Code and &#xA;&#x9;&#x9;  ISNULL(od.size_code,'')= ISNULL(pds.size_code,'') and ISNULL(od.Color_code,'') = ISNULL(pds.Color_code,'')&#xA;&#x9;&#x9;  and o.DT_Order between pds.DT_Effective and pds.DT_End +1&#xA;&#x9;left join &#xA;&#x9;&#x9;  #temp_dms_orders t on o.Order_ID = t.order_id&#xA;&#x9;where &#xA;&#x9;&#x9;  (mbe.Benop_Value = 'H2226-001' OR MBE.Benop_Value = 'DMS_MA')&#xA;&#x9;and &#xA;&#x9;&#x9;  m.HIC_Number is not null &#xA;&#x9;and &#xA;&#x9;&#x9;  gc.Pcp_id is not null&#xA;&#x9;and &#xA;&#x9;&#x9;o.DT_Print between ? and ? &#xA;&#x9;and &#xA;&#x9;&#x9;o.DT_Print is not null&#xA;&#xA;&#xA;drop table #temp_dms_orders&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
