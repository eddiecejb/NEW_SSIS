                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @EndDate datetime 
select 
		   m.external_member_id [SUBSCRIBERNUMBER]
		  ,mc.Group_code [GROUPNUMBER]
		  ,m.First_Name [FNAME], m.Middle_Name [MI]
		  ,m.Last_Name [LNAME]
		  ,mal.Address_1 [ADDRESS1]
		  ,mal.Address_2 [ADDRESS2]
		  ,mal.City [CITY]
		  ,mal.State [STATE]
		  ,mal.Zip
		  ,dbo.fn_GetMemberCreditBalanceMonthEnd(m.Member_ID,bp.Benefit_Cap,mb.DT_Effective,mb.DT_End, @EndDate) AS [BALANCE]
    from
	   Members m (NOLOCK)
    left join 
	   Members_Group_Codes mc (NOLOCK) on m.External_Member_ID = mc.External_Member_Id 
    left join 
	   Member_Address_List mal (NOLOCK) on m.Current_Address_ID = mal.Address_ID 
    left join 
	   Benefit_Plan_Mapping bpm (NOLOCK) on bpm.Eligibility_Benefit_Code = mc.Group_code 
	   and GETDATE() between bpm.DT_Effective and bpm.DT_End + 1
    left join 
	   Benefit_Plans bp on bpm.Plan_Code = bp.Plan_Code 
	   and GETDATE() between bp.DT_Effective and bp.DT_End + 1
    cross apply 
	   (select top 1 DT_Effective, DT_End from Member_Benefit_Events mbe where mbe.Member_ID = m.Member_ID 
	    and GETDATE() between DT_Effective and DT_End+1) mb
    where 
	   mc.Group_code in 
	(   
	   select Eligibility_Benefit_Code from Benefit_Plan_Mapping 
	   where Plan_Code in (select Plan_Code from Benefit_Plans 
	   where GETDATE() between DT_Effective and DT_End 
	   and (Source_Code like '%IA%' or Source_Code like '%EA%'))
	)
	and 
	m.status = 'Active'</property>
