              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select
     pds.Dropship_Account_No [Account] 
    ,case when i.invoice_number Is null then 'Ship Unconfirmed' else i.Invoice_Number end [Invoice Number]
    ,o.Order_ID
    ,o.dt_ship [Ship Date]
    ,o.dt_order [Order Date]
    ,pds.Drop_Ship_Item_Code as [Item No]
    ,p.product_name + ' ' + isnull(pds.size_code,'') + ' ' + isnull(pds.color_code,'')  + '' + ISNULL(od.flavor,'') as [Item Description]
    ,case when pds.Quantity IS NOT NULL then od.Quantity_Shipped * pds.Quantity ELSE od.Quantity_Shipped end [Qty Shipped]
    ,case when pds.Quantity IS NOT NULL then pds.Shipping_Conversion_UOM else p.packing_type end as UOM
    ,cast(pds.unit_cost as money) as Price
    ,case when pds.Quantity IS NOT NULL then cast((od.quantity_shipped * (pds.Quantity * pds.Unit_Cost)) as money) 
	 else cast((od.Quantity_Shipped * pds.Unit_Cost) as money) end as [Ext Price]	    
from
order_details od 
left join 
orders o on o.order_id = od.order_id
left join
products p on od.product_id = p.product_id
left join 
product_dropship_item_options pds on p.product_code = pds.product_code 
and isnull(od.size_code,'') = isnull(pds.size_code,'') and isnull(od.color_code,'') = isnull(pds.color_code,'') and ISNULL(od.flavor,'') = ISNULL(pds.Flavor_Option,'')
and o.dt_print between pds.dt_effective and pds.dt_end+1
OUTER apply 
    (select top 1 invoice_number from shipping_confirmation_history h where o.order_id = h.order_id) i
where o.dt_print between ? and ?
order by o.dt_order, o.order_id</property>
