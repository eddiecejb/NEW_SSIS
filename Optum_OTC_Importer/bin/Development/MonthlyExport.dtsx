              description="OLE DB Source"
              name="DMS Sales Query"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
    o.order_id as ORDERNO,
    m.member_id as CUSTNO,
    o.DT_Created as ENTRYDATE,
    o.DT_Ship as SHIPDATE,
    convert(varchar,o.dt_print,101) DT_EXPORT,
    CASE WHEN fcm.state IN ('FL', 'MA', 'KS') THEN fcm.state + bp.Source_Code 
		ELSE o.Ship_To_State + bp.Source_Code END 
     as SOURCE_CODE,
    fmn.Flm_Market_Name as MARKETNAME,
    upper(fcm.first_name) + ' ' + upper(fcm.last_name) as NAMEEX,
    upper(rtrim(m.First_Name)) as FNAME,
    upper(rtrim(m.Last_Name)) as LNAME,
    pds.hcpc_code as HCPC_CODE,
    p.Catalog_Number + ' ' + isnull(pds.Size_Code,'') as ITEMNO,
    p.Product_Name ITEM_DESCRIPTION,
    CASE WHEN pds.Shipping_Conversion_Item_No IS NOT NULL THEN pds.Quantity else od.Quantity_Shipped end SUMOFITEM_QTY,
    od.Quantity_Shipped * p.Total_Member_Cost SUMOF_ITEM_PRICE,
    CASE WHEN pds.Shipping_Conversion_Item_No IS NOT NULL THEN od.quantity_Shipped  * (pds.Quantity * pds.Unit_Cost) else od.Quantity_Shipped * pds.unit_cost end SUMOF_EXTCOST,
    --od.Quantity_Shipped SUMOFITEM_QTY,
    --p.Total_Member_Cost SUMOF_ITEM_PRICE,
    --od.quantity_Shipped * p.Total_Member_Cost SUMOF_EXTPRICE,
    'U' as BIG_STATUS,
    o.Ship_To_State as STATE,
    o.Ship_To_State as DEMOGRAPHICS,
    o.Ship_To_State as GROUP_NO,
    o.Ship_To_State as LOCATION
    
from	   
    orders o (nolock)
left join 
    Order_Details od (nolock) on o.order_id = od.order_id 
left join 
    Products p on od.Product_ID = p.Product_ID
join 
    Product_Dropship_Item_Options pds on p.Product_Code = pds.Product_Code 
    and ISNULL(od.Size_Code,'') = ISNULL(pds.size_code,'') 
    and ISNULL(od.Color_Code,'') = ISNULL(pds.Color_code,'') 
    and o.dt_print between pds.DT_Effective and pds.DT_End
left join 
    Member_Benefit_Events mbe (nolock) on o.Benefit_Event_ID = mbe.Benefit_Event_ID
left join 
    Benefit_Plans bp (nolock) on mbe.Benop_Value = bp.Plan_Code and o.DT_Created between bp.DT_Effective and bp.DT_End
left join 
    Members m (nolock) on o.Member_ID = m.Member_ID 
left join 
    Flm_CareManagers fcm (nolock) on fcm.otc_user_id = o.Created_By_ID    
left join 
    Flm_Market_Names fmn (nolock) on fmn.flm_Source_Code = CASE WHEN fcm.state IN ('FL', 'MA', 'KS') THEN fcm.state + bp.Source_Code 
											         ELSE o.Ship_To_State + bp.Source_Code END 
where
    bp.division = 3 
and
    o.DT_PRINT between ? and ?
order by o.Order_ID</property>
