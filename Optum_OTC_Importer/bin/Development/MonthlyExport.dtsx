      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{3E623206-18B8-4D6D-9AFA-389A64C0BAFC}"
          SQLTask:SqlStatementSource="    select distinct member_id, rgc.group_code, rgc.PCP_ID into #recentGC from download_members_history dmh (nolock)&#xA;    cross apply &#xA;&#x9;   (select top 1 Group_code, PCP_ID from Download_Members_History gc (nolock) where gc.Member_ID = dmh.Member_ID &#xA;&#x9;   and gc.processing_status = 'COMPLETE' and Download_ID = (select MAX(download_id) from Download_Members_History did (nolock) &#xA;&#x9;   where did.Member_ID = dmh.Member_ID and Processing_Status = 'COMPLETE')) rgc&#xA;    &#xA;    update m set m.group_code = r.Group_code, m.Pcp_id = r.PCP_ID from &#xA;    Members_Group_Codes m, #recentGC r where &#xA;    m.External_Member_Id = r.Member_ID&#x9;   &#xA;    &#xA;    insert into Members_Group_Codes&#xA;    select distinct r.Member_id, r.Group_Code, m.member_id, r.PCP_ID, '06/01/2016','12/31/2099',GETDATE()  from #recentGC r &#xA;    join &#xA;    Members m (nolock) on r.Member_ID = m.External_Member_ID where &#xA;    r.Member_ID not in &#xA;    (select External_Member_ID from Members_Group_Codes (nolock) where External_Member_ID is not null)&#xA;    and &#xA;    m.Member_ID is not null&#xA;    &#xA;    DROP TABLE #recentGC" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
