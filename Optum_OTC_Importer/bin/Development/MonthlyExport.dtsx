                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 
       gc.Group_code [GROUP_NBR]
      ,o.SHIP_TO_CITY [CITY]
      ,mal.[State] [dbo_CUSTOMERS#STATE]
      ,o.ship_to_State [Sales Tax#STATE]
      ,CASE 
	   WHEN bp.division = 3 and pcm.div_03_code IS NOT NULL THEN pcm.div_03_code 
	   WHEN bp.Division = 6 and pcm.div_06_code IS NOT NULL THEN pcm.div_06_code
	   WHEN bp.Division = 5 and pcm.div_05_code IS NOT NULL THEN pcm.div_05_code
	   WHEN bp.Division = 8 and pcm.div_08_code IS NOT NULL THEN pcm.div_08_code
	   WHEN bp.division = 0 and bp_div.Division = 3 THEN pcm.div_03_code 
	   WHEN bp.Division = 0 and bp_div.Division = 6 THEN pcm.div_06_code
	   WHEN bp.Division = 0 and bp_div.Division = 5 THEN pcm.div_05_code
	   WHEN bp.Division = 0 and bp_div.Division = 8 THEN pcm.div_08_code
	   ELSE cast(p.Catalog_Number  as varchar) 
	  END as [ITEMNO]
      ,p.Product_Name + ISNULL(od.Size_Code,'') + ' ' + ISNULL(od.color_code,'') [ITEM_DESCRIPTION]
      ,CASE WHEN pds.Shipping_Conversion_Item_No IS NOT NULL THEN pds.Quantity * od.Quantity_Shipped else od.Quantity_Shipped end [SumOfSumOfITEM_QTY]
      ,od.Quantity_Shipped * p.Total_Member_Cost [SumOfSumOfEXT_PRICE]
      ,CASE WHEN pds.Shipping_Conversion_Item_No IS NOT NULL THEN od.Quantity_Shipped * (pds.Quantity * pds.Unit_Cost) else od.Quantity_Shipped * pds.unit_cost end [SumOfSumOfEXT_COST]
from
    orders o (nolock)
left join 
    Order_Details od (nolock) on o.order_id = od.order_id 
left join 
    Products p on od.Product_ID = p.Product_ID
left join 
    Product_Dropship_Item_Options pds on p.Product_Code = pds.Product_Code 
    and 
    ISNULL(od.Size_Code,'') = ISNULL(pds.Size_Code,'') and ISNULL(od.Color_code,'') = ISNULL(pds.color_code,'')
    and 
    o.dt_print between pds.DT_Effective and pds.DT_End + 1
left join 
    Member_Benefit_Events mbe (nolock) on o.Benefit_Event_ID = mbe.Benefit_Event_ID
left join 
    Benefit_Plans bp (nolock) on mbe.Benop_Value = bp.Plan_Code and o.DT_Created between bp.DT_Effective and bp.DT_End
left join 
    Product_Catalog_Mappings pcm on p.Product_Code = pcm.product_code
outer apply 
    (select top 1 group_code from Members_Group_Codes mgc where mgc.Member_id = o.Member_ID) gc
outer apply 
    (select top 1 bp2.Division from Benefit_Plan_Mapping (nolock) bpm left join benefit_plans bp2 (nolock) on bpm.plan_code = bp2.plan_code where gc.group_code = Eligibility_Benefit_Code) bp_div    
left join 
    Members m (nolock) on o.Member_ID = m.Member_ID 
left join 
    member_address_list mal on m.Current_Address_id = mal.Address_id 
left join 
  svw_AppSec_Users u on o.Created_By_ID = u.AppSec_Users_ID
  where 
  bp.division NOT IN (3,9)
  and 
  bp.Plan_Code not in 
  (select distinct Plan_Code from Benefit_Products b (nolock) join benefit_plans bpp (nolock) on b.benefit_plan_id = bpp.benefit_plan_id where b.DT_Effective = '02/01/2016')
  and o.dt_print between '03/01/2016' and '04/01/2016' order by o.Order_ID</property>
