                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 
	    case when isNull(m.HIC_Number,'') = '' THEN m.External_Member_ID ELSE m.HIC_Number END as HIC
	   ,convert(varchar,o.DT_Order,101) + ' 00:00:00' as [Date of Service]
	   ,'"Order of Product(s)"' as [Service Type]
	   ,'"1"' as [Service Count]
	   ,'"' + CASE WHEN isNULL(o.Total_Item_Cost,0) = 0 then cast(cast(dbo.fn_GetOrderTotal(o.order_id) as int) As varchar(5)) else cast(cast(o.Total_Item_Cost as int) as varchar(5)) END + '"' as [Member Spend per Service]
    FROM
	   Orders o 
    LEFT JOIN 
	   Members m on o.Member_ID = m.Member_ID
    LEFT JOIN 
	   Member_Benefit_Events mbe on o.Benefit_Event_ID = mbe.Benefit_Event_ID
    JOIN 
	   Benefit_Plans bp on mbe.Benop_Value = bp.Plan_Code 
    WHERE 
	   bp.Division in (5,6,9)
    AND    
                     mbe.benop_value &lt;&gt; 'H2226-001'
    AND
	   o.Order_Status = 'SHIPPED'
    AND 
	   o.DT_Ship between ? and ?
