      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{3E623206-18B8-4D6D-9AFA-389A64C0BAFC}"
          SQLTask:SqlStatementSource="insert into Staging_Order_Details_Export&#xA;select &#xA;    bp.Division as DIVISION,&#xA;    m.member_id as CUSTEDP,&#xA;    o.order_id as ORDERNO,&#xA;    bp.source_code AS SOURCE_CODE, &#xA;    convert(varchar, o.dt_print, 101) as SHIPDATE,&#xA;    CASE &#xA;                   WHEN pv.Member_Cost Is not null then od.Quantity_Shipped * pv.Member_Cost&#xA;                   ELSE od.Quantity_Shipped * p.Total_Member_Cost&#xA;    END as [SumOfEXT_PRICE],&#xA;    CASE &#xA;                   WHEN pds.Shipping_Conversion_Item_No Is not null THEN &#xA;                                  (od.Quantity_Shipped * pds.Quantity) * pds.unit_cost&#xA;                   ELSE &#xA;                                  od.Quantity_Shipped * pds.Unit_Cost &#xA;                END as SumOfEXT_COST,&#xA;    ISNULL(mg.Group_code,'OPTUM') as [GROUP#],&#xA;    NULL AS [C/I],&#xA;    mal.[State] as [State],&#xA;    bp.plan_code as HPlan&#xA;from &#xA;    Orders o (nolock)&#xA;left join &#xA;    Order_Details od (nolock) on o.Order_ID = od.Order_ID &#xA;left join &#xA;    Products p on od.Product_ID = p.Product_ID &#xA;cross apply (select top 1  Product_Dropship_Item_Option_ID, Shipping_Conversion_Item_No, Quantity, Unit_Cost    from &#xA;    Product_Dropship_Item_Options pds (nolock) where p.Product_Code = pds.Product_Code     &#xA;    and ISNULL(od.Size_Code,'') = ISNULL(pds.size_code,'')     &#xA;    and ISNULL(od.Color_Code,'') = ISNULL(pds.Color_code,'')    &#xA; and ISNULL(od.flavor,'') = ISNULL(pds.Flavor_Option,'')      &#xA;    and o.DT_print between pds.DT_Effective and pds.DT_End + 1  ) pds&#xA;left join &#xA;    Member_Benefit_Events mbe (nolock) on o.Benefit_Event_ID = mbe.Benefit_Event_ID &#xA;left join &#xA;    Benefit_Plans bp (nolock) on mbe.Benop_Value = bp.Plan_Code &#xA;                   and cast(convert(varchar(15),o.dt_created,101) as datetime) between bp.DT_Effective and bp.DT_End &#xA;left join &#xA;    members m (nolock) on o.Member_ID = m.Member_ID &#xA;outer apply &#xA;                (select top 1 group_code from &#xA;    Members_Group_Codes mgc (nolock) where m.member_id = mgc.member_id and DT_Effective = (select MAX(DT_Effective) from members_group_codes mgc2 (nolock) where mgc2.member_id = m.Member_ID)) mg&#xA;left join &#xA;    Member_Address_List mal (nolock) on m.Current_Address_ID = mal.Address_ID&#xA;left join &#xA;    dbo.Product_Dropship_Member_Price_Varianaces pv (nolock) on p.Product_ID = pv.Product_Id&#xA;    and pv.Dropship_Item_Option_id = pds.Product_Dropship_Item_Option_ID&#xA;where &#xA;&#x9;&#x9;o.dt_print between ? and ?&#xA;and &#xA;   ISNULL(bp.Division,0) &lt;&gt; 3&#xA;and o.order_id not in (select  order_id  from TEMP_MAIL_ORDERS_2017_CARRYOVER)&#xA;&#xA;    &#xA;select MAX(benefit_event_id) benefit_event_id, Member_ID, cast('' as varchar(20)) BenopValue into #tempBenop from &#xA;Member_Benefit_Events where Benop_Value &lt;&gt; '000' and Member_ID in &#xA;(select CUSTEDP from Staging_Order_Details_Export where ISNULL(DIVISION,0) = 0) &#xA;group by Member_ID &#xA;&#xA;&#xA;&#xA;update t set t.BenopValue = mbe.Benop_Value&#xA;from #tempBenop t, Member_Benefit_Events mbe &#xA;where t.benefit_event_id = mbe.Benefit_Event_ID&#xA;&#xA;select t.*, bp.Division, bp.Source_Code, bp.plan_code into #tempUpdated from #tempBenop t &#xA;join &#xA;Benefit_Plans bp on t.BenopValue = bp.Plan_Code &#xA;and dt_end &gt;= convert(varchar,getdate(), 101)&#xA;&#xA;Insert into #tempUpdated&#xA;select temp.*, bp.Division, bp.Source_Code, bp.plan_code&#xA;from #tempBenop temp&#xA;left join #tempUpdated t on t.benefit_event_id = temp.benefit_event_id&#xA;join benefit_plans bp on temp.BenopValue = bp.plan_code&#xA;and DT_End = ( select max(dt_end) from benefit_plans bp1 where bp1.Plan_Code=bp.Plan_Code)&#xA;where isNull(t.Division,0) = 0&#xA;&#xA;update s&#xA;set s.DIVISION = t.division, s.source_code = t.source_code, s.HPLAN = t.Plan_Code&#xA;FROM &#xA;Staging_Order_Details_Export s, #tempUpdated t&#xA;where &#xA;s.CUSTEDP = t.Member_ID &#xA;and &#xA;ISNULL(s.DIVISION,0) = 0&#xA;&#xA;&#xA;update S&#xA;set s.DIVISION= bp.Division&#xA;from  Staging_Order_Details_Export S&#xA;join benefit_plans bp on s.HPLAN=BP.Plan_Code&#xA;and getdate() between DT_Effective and DT_End&#xA;                &#xA;drop table #tempBenop&#xA;drop table #tempUpdated&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
